    {
        "version": 2,
        "builds": [
            {
                "src": "referral/wsgi.py",
                "use": "@vercel/python",
                "config": {
                    "maxLambdaSize": "15mb"
                }
            }
        ],
        "routes": [
            {
                "src": "/(.*)",
                "dest": "referral/wsgi.py"
            }
        ]
    }
